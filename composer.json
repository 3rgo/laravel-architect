{
    "name": "3rgo/laravel-architect",
    "description": "Laravel preset manager",
    "keywords": [
        "laravel",
        "installer",
        "preset",
        "laravel-installer",
        "boilerplate",
        "scaffold"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain GONÃ‡ALVES",
            "email": "romain@3rgo.tech"
        }
    ],
    "require": {
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laravel/installer": "^5.11",
        "laravel/prompts": "^0.3.4",
        "symfony/console": "^6.2|^7.0",
        "symfony/process": "^6.2|^7.0"
    },
    "bin": [
        "bin/architect"
    ],
    "autoload": {
        "psr-4": {
            "LaravelArchitect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelArchitect\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
